    if (confirmSendOut()) {
        sendFromServer();
    }






    function confirmSendOut() {

		
		
        var answer = confirm("Please confirm you want email sent out.");
        if (answer == true) {
            alert("YES");
            return true;
        } else {
            alert("NO");
            return false;
        }
    }

    function sendFromServer() {
        var ga = new GlideAjax('x_673829_service_c.exampleSkuInclude');
        ga.addParam('sysparm_name', 'sendOutEmail');
        ga.getXML(emailSent);

        function emailSent(response) {
            alert("Done?");
        }

    }
}]]></script>
